{
  "name": "theofficialvowswap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "cypress run",
    "test:a11y": "cypress run --spec 'cypress/e2e/accessibility.spec.js'",
    "test:perf": "node tests/performance/lighthouse.js",
    "test:load": "k6 run tests/performance/load-test.js",
    "test:load:checkout": "k6 run tests/performance/load-test.js --env SCENARIO=checkout",
    "test:load:realtime": "k6 run tests/performance/load-test.js --env SCENARIO=realtime",
    "test:visual": "percy exec -- cypress run",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:a11y && npm run test:perf",
    "validate:env": "node scripts/validate-env.js",
    "generate:icons": "node scripts/generate-pwa-icons.js",
    "predev": "prisma generate",
    "prepare": "husky install",
    "security:audit": "npm audit --audit-level=high",
    "security:snyk": "snyk test",
    "security:check": "npm run security:audit && npm run security:snyk"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.3.0",
    "@heroicons/react": "^2.3.0",
    "@next-auth/prisma-adapter": "^1.1.0",
    "@prisma/client": "^6.4.0",
    "@radix-ui/react-dropdown-menu": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.11.0",
    "@supabase/ssr": "^0.6.0",
    "@supabase/supabase-js": "^2.50.0",
    "@tremor/react": "^3.19.0",
    "@types/bcryptjs": "^2.4.7",
    "@types/react-color": "^3.0.14",
    "@upstash/ratelimit": "^2.1.0",
    "@upstash/redis": "^1.35.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.2",
    "date-fns": "^4.2.0",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.2.0",
    "framer-motion": "^12.5.0",
    "ioredis": "^5.6.0",
    "lucide-react": "^0.475.0",
    "next": "15.2.0",
    "next-auth": "^4.25.0",
    "next-pwa": "^5.7.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.4.0",
    "react-color": "^2.19.4",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.1.0",
    "react-focus-lock": "^2.14.0",
    "react-hot-toast": "^2.5.2",
    "react-input-mask": "^2.0.4",
    "react-range": "^1.10.0",
    "recharts": "^2.16.0",
    "socket.io": "^4.9.0",
    "socket.io-client": "^4.9.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@percy/cli": "^1.31.0",
    "@percy/cypress": "^3.2.0",
    "@testing-library/dom": "^10.5.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.1.0",
    "@testing-library/user-event": "^14.7.0",
    "@types/firebase": "^2.4.32",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.15",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-input-mask": "^3.0.7",
    "axe-core": "^4.11.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.4.0",
    "msw": "^2.8.0",
    "postcss": "^8.4.0",
    "prisma": "^6.4.0",
    "sharp": "^0.33.6",
    "snyk": "^1.1250.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.0"
  }
}
